<div class="employee-list transparent-box">
  <table>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Account Status</th>
      <th>Schedule</th>
    </tr>
    <% @employees.each do |employee| %>
    <% next if employee.id == current_user.id %>
      <tr class="employee-info-row">
          <td><%= employee.first_name %></td>
          <td><%= employee.last_name %></td>
          <td><%= employee.email %></td>
          <td><%= employee.account_status %></td>
          <td>TBD</td>
      </tr>
    <%end %>
  </table>
</div>